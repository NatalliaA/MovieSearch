<h1>MOVIES</h1>
<p>You searched for <strong><%= data.query %></strong>:</p>
<ol>
<% data.searchres.forEach(function(movie){ %>
        <li>
            <strong><%= movie.Title %></strong> - <%= movie.Year %>            
        </li>
        <img alt="poster not available" src=<%= movie.Poster %> height="100px">
    <% }) %>
    </ol>

    <a href="/">Search Again</a>
